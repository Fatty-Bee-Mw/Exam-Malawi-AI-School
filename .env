# ==================================
# Exam AI Malawi - Environment Configuration
# ==================================
# Copy this file to .env and update with your actual values
# Never commit .env file to version control

# AI API Configuration (Required for production)
REACT_APP_AI_API_URL=https://your-ai-api-endpoint.com
REACT_APP_AI_API_KEY=your-ai-api-key-here

# Optional: Analytics and Monitoring
REACT_APP_ANALYTICS_ID=your-analytics-id

# Optional: Payment Processing (for premium features)
REACT_APP_PAYMENT_API_KEY=your-payment-api-key

# Backend API Base URL (FastAPI server)
REACT_APP_API_BASE_URL=http://localhost:8000

# Environment Mode (development, production, test)
NODE_ENV=development

# Enable/Disable Features
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_PAYMENT=false
REACT_APP_DEBUG_MODE=true
DISABLE_ESLINT_PLUGIN=true

