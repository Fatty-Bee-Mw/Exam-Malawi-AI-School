{
  "name": "exam-ai-malawi",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "kill-servers": "node kill-servers.js",
    "dev": "node kill-servers.js && npm start",
    "serve": "node kill-servers.js && serve -s build -p 3000",
    "clean": "rimraf build node_modules/.cache",
    "fresh-install": "npm run clean && npm install",
    "fresh-start": "npm start",
    "start-backend": "cd backend && python app.py",
    "start-all": "concurrently \"npm run start-backend\" \"npm run dev\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "typescript": "^4.9.5",
    "rimraf": "^5.0.5",
    "serve": "^14.2.1",
    "concurrently": "^8.2.2"
  }
}
